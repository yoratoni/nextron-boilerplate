    "devDependencies": {
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/node": "^20.8.6",
        "@types/react": "^18.2.45",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "@typescript-eslint/parser": "^6.7.5",
        "concurrently": "^8.2.2",
        "eslint": "^8.51.0",
        "eslint-import-resolver-typescript": "^3.6.1",
